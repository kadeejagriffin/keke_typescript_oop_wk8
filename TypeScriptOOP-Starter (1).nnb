        {
            "language": "typescript",
            "source": [
                "class Duck {\n    public static className:string = 'Duck'\n    \n    constructor(protected age$:number){}\n    \n    get age():number{\n        return this.age$\n    }\n    \n    public quack():void{\n        console.log('Generic Quack Sound')\n    }\n    \n    public swim():void{\n        console.log('Paddles with two feet')\n    }\n}\nclass MallardDuck extends Duck {\n    public static className:string = 'MallardDuck' // override the Duck className property\n    \n    constructor(public color:string, age:number){\n        super(age)\n    }\n\n    // Let's pretenf that Mallard Ducks are ALWAYS 5 years older than they claim\n    public get age():number{\n        return this.age$ +5\n    }\n\n    // Let's also ovveride the quack method to have it log a Mallard's quack\n    public quack():void{\n        console.log('Mallard Quack!!!!')\n    }\n}"
